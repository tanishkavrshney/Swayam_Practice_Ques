import java.util.* ;
import java.io.*; 
import java.util.ArrayList;

public class Solution 
{
    public static void reverseArray(ArrayList<Integer> arr, int m)
    {
        // Write your code here.
        for(int i=0;i<=m;i++){
            System.out.print(arr.get(i)+" ");
        }
        for(int i=arr.size()-1;i>m;i--){
            System.out.print(arr.get(i)+" ");
        }
        System.out.println();
    }
}
