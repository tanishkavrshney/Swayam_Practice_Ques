class Solution {
    public int reverseExponentiation(int n) {
        // code here
        if(n==10) return 10;
        int on=n;
        int a=0;
        int rev=0;
        if(n<10) rev=n;
        else{
            while(n>=10){
                a=n%10;
                rev=rev+a*10;
                n=n/10;
            }
        }
        int prod=1;
        for(int i=1;i<=rev;i++){
            prod=prod*n;
        }
        return prod;
    }
}
